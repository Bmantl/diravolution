{
  "name": "Appartment",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "location": {
      "type": "geopoint",
      "required": true
    },
    "discussion":{
      "type": "hasOne",
      "model": "discussion",
      "foreignKey": "appartmentId"
    },
    "tickets":{
      "type": "hasMany",
      "model": "ticket",
      "foreignKey": "appartmentId"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": []
}
